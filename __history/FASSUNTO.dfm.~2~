  object qryAssunto: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO CHAT0200'
      '  (COD_SUB_ASSUNTO, SUB_ASSUNTO, COD_ASSUNTO, ORDEM)'
      'VALUES'
      '  (:COD_SUB_ASSUNTO, :SUB_ASSUNTO, :COD_ASSUNTO, :ORDEM)')
    SQLDelete.Strings = (
      'DELETE FROM CHAT0200'
      'WHERE'
      
        '  COD_SUB_ASSUNTO = :Old_COD_SUB_ASSUNTO AND COD_ASSUNTO = :Old_' +
        'COD_ASSUNTO')
    SQLUpdate.Strings = (
      'UPDATE CHAT0200'
      'SET'
      
        '  COD_SUB_ASSUNTO = :COD_SUB_ASSUNTO, SUB_ASSUNTO = :SUB_ASSUNTO' +
        ', COD_ASSUNTO = :COD_ASSUNTO, ORDEM = :ORDEM'
      'WHERE'
      
        '  COD_SUB_ASSUNTO = :Old_COD_SUB_ASSUNTO AND COD_ASSUNTO = :Old_' +
        'COD_ASSUNTO')
    SQLLock.Strings = (
      'SELECT * FROM CHAT0200'
      'WHERE'
      
        '  COD_SUB_ASSUNTO = :Old_COD_SUB_ASSUNTO AND COD_ASSUNTO = :Old_' +
        'COD_ASSUNTO'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      
        'SELECT COD_SUB_ASSUNTO, SUB_ASSUNTO, COD_ASSUNTO, ORDEM FROM CHA' +
        'T0200'
      'WHERE'
      
        '  COD_SUB_ASSUNTO = :COD_SUB_ASSUNTO AND COD_ASSUNTO = :COD_ASSU' +
        'NTO')
    SQL.Strings = (
      'SELECT *'
      '  FROM CHAT0200'
      ' ORDER BY ORDEM')
    FetchAll = True
    Left = 184
    Top = 136
    object qryAssuntoCOD_ASSUNTO: TFloatField
      DisplayLabel = 'C'#243'digo'
      FieldName = 'COD_ASSUNTO'
      Required = True
    end
    object qryAssuntoASSUNTO: TStringField
      DisplayLabel = 'Processo'
      FieldName = 'ASSUNTO'
      Required = True
      Size = 50
    end
    object qryAssuntoORDEM: TFloatField
      FieldName = 'ORDEM'
    end
  end
  object qryCodigo: TOraQuery
    SQL.Strings = (
      'SELECT NVL(MAX(COD_SUB_ASSUNTO),0)+1 AS COD_SUB_ASSUNTO,'
      '       NVL(MAX(ORDEM),0)+1 AS ORDEM '
      '  FROM CHAT0200')
    FetchAll = True
    Left = 176
    Top = 192
    object qryCodigoCOD_SUB_ASSUNTO: TFloatField
      FieldName = 'COD_SUB_ASSUNTO'
    end
    object qryCodigoORDEM: TFloatField
      FieldName = 'ORDEM'
    end
  end
